<div class="container-fluid roadmap-container">
    <div class="row">
        <div class="col-md-10 offset-md-1">

        </div>
    </div>
    <div class="row">
        <div class="col-md-10 offset-md-1">
            <br>
        </div>
    </div>
    <div class="row">
        <div class="col-md-2 offset-md-1">
            <div class="tag prep">Prep</div>
            <div class="big_number txt-prep">{{ counts.Prep }}</div>
        </div>
        <div class="col-md-2">
            <div class="tag discovery">Discovery</div>
            <div class="big_number txt-discovery">{{ counts.Discovery }}</div>
        </div>
        <div class="col-md-2">
            <div class="tag alpha">Alpha</div>
            <div class="big_number txt-alpha">{{ counts.Alpha }}</div>
        </div>
        <div class="col-md-2">
            <div class="tag beta">Beta</div>
            <div class="big_number txt-beta">{{ counts.Beta }}</div>
        </div>
        <div class="col-md-2">
            <div class="tag live">Live</div>
            <div class="big_number txt-live">{{ counts.Live }}</div>
        </div>
    </div>

</div>
{{#each projects}}
    <div class="row">
        <div class="col-md-10 offset-md-1">
            <h2 class="heading-large service-area">{{@key}}</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-md-2 offset-md-1">
            <section class="column-fifth">
                <ul class="link-list border-beta">
                    {{#each this.Prep}}
                            <li class="list-item"><a href="/project/{{id}}">{{name}}</a></li>
                    {{/each}}
                </ul>
                <ul class="link-list border-internal"></ul>
            </section>
        </div>
        <div class="col-md-2">
            <section>
                <ul class="link-list border-beta">
                    {{#each this.Discovery}}
                            <li class="list-item"><a href="/project/{{id}}">{{name}}</a></li>
                    {{/each}}
                </ul>
                <ul class="link-list border-internal"></ul>
            </section>
        </div>
        <div class="col-md-2">
            <section>
                <ul class="link-list border-beta">
                    {{#each this.Alpha}}
                            <li class="list-item"><a href="/project/{{id}}">{{name}}</a></li>
                    {{/each}}
                </ul>
                <ul class="link-list border-internal"></ul>
            </section>
        </div>
        <div class="col-md-2">
            <section>
                <ul class="link-list border-beta">
                    {{#each this.Beta}}
                            <li class="list-item"><a href="/project/{{id}}">{{name}}</a></li>
                    {{/each}}
                </ul>
                <ul class="link-list border-internal"></ul>
            </section>
        </div>
        <div class="col-md-2">
            <section>
                <ul class="link-list border-beta">
                    {{#each this.Live}}
                            <li class="list-item"><a href="/project/{{id}}">{{name}}</a></li>
                    {{/each}}
                </ul>
                <ul class="link-list border-internal"></ul>
            </section>
        </div>
    </div>
{{/each}}
</div>



