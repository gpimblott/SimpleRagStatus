{
  "name": "RAGe",
  "version": "0.1.0",
  "scripts": {
    "start": "node -r esm ./bin/www.js",
    "circleci-test": "mocha tests/*.js --require esm --reporter xunit --reporter-options output=./tests/test-results.xml --exit",
    "test": "mocha tests/*.js --require esm  --exit",
    "dev": "nodemon -r esm ./bin/www.js",
    "migrate-up": "db-migrate up",
    "migrate-down": "db-migrate down",
    "create-test-data": "node -r esm ./scripts/createTestData"
  },
  "engines": {
    "node": "12.4"
  },
  "keywords": [
    "Node.js",
    "application"
  ],
  "description": "Project RAG and Risk logging application",
  "author": "gordon-pimblott",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "latest",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "debug": "latest",
    "dotenv": "^8.1.0",
    "esm": "^3.2.25",
    "exceljs": "^3.3.0",
    "express": "^4.17.3",
    "express-enforces-ssl": "latest",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.16.2",
    "fast-csv": "^3.4.0",
    "handlebars-dateformat": "^1.1.1",
    "helmet": "^3.21.0",
    "morgan": "latest",
    "multer": "^1.4.2",
    "node-cache": "latest",
    "owasp-password-strength-test": "^1.3.0",
    "passport": "latest",
    "passport-local": "latest",
    "pg": "^7.12.1",
    "serve-favicon": "latest",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.2",
    "mocha-xunit-reporter": "^1.1.0",
    "nodemon": "^1.19.4"
  }
}
